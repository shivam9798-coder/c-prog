#include <stdio.h>
int main()
{
    int ch;
    printf("enter the any letter");
    scanf("%c",&ch);
    switch(ch)
    {
    case 'a':
    case 'A':
    case 'e':
    case 'E':
    case 'i':
    case 'I':
    case 'o':
    case 'O':
    case 'u':
    case 'U':
   printf("%c",ch);
   break;
   default : printf("not vovle");
   }
return 0; 
}
